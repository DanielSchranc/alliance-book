{
  "name": "alliance-book-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "clean": "rm -rf dist",
    "clean:cache": "npm cache clean --force",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
