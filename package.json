    "formatter:check": "prettier --check .",
    "formatter:write": "prettier --write ."
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
